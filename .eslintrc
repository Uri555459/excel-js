{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "require-jsdoc": "off",
    "eol-last": "off",
    "object-curly-spacing": ["error", "always"],
    "quotes": ["error"]
  },
  "extends": ["eslint:recommended", "google"]
}